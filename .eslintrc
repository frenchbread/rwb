{
  "parser": "babel-eslint",
  "parserOptions": {
    "allowImportExportEverywhere": true,
    "ecmaVersion": 7
  },
  "plugins": [],
  "extends": [
    "airbnb-base",  ],
  "settings": {},
  "rules": {
    "comma-dangle": [
      2,
      "always-multiline"
    ],
    "import/no-extraneous-dependencies": 0,
    "space-before-function-paren": [
      2,
      {
        "anonymous": "always",
        "named": "always"
      }
    ],
    "no-underscore-dangle": [
      2,
      {
        "allow": ["_id", "__"]
      }
    ]
  }
}
